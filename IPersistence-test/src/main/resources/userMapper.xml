<mapper namespace="org.maxwell.lg.dao.UserDao">


    <select id="findAll" resultType="org.maxwell.lg.entity.User">
        select * from user
    </select>


    <select id="findOne" resultType="org.maxwell.lg.entity.User" parameterType="org.maxwell.lg.entity.User">
        select * from user where id =#{id} and name = #{name}
    </select>


</mapper>